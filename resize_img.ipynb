    "from PIL import Image\n",
    "import glob"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "imgs = glob.glob('./imgs/*.png')\n",
    "\n",
    "for img in imgs:\n",
    "    img_orig = Image.open(img)\n",
    "    img_resized = img_orig.resize((360, 180)).crop((5, 0, 280, 137))\n",
    "    img_resized.save(img)"
   ]
  },
